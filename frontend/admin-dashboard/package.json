{
  "name": "peerai-admin-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview --host --port $PORT",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--loader ts-node/esm' cypress run",
    "test:open": "NODE_OPTIONS='--loader ts-node/esm' cypress open",
    "test:ci": "start-server-and-test dev http://localhost:3000 test",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@tanstack/react-query": "^5.24.1",
    "axios": "^1.6.7",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.5.0",
    "react-json-view-lite": "^2.3.0",
    "react-router-dom": "^6.22.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^10.0.4",
    "recharts": "^2.12.1",
    "redoc": "^2.4.0",
    "swagger-ui-react": "^5.19.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cypress": "^0.1.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "cypress": "^14.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "start-server-and-test": "^2.0.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "webpack": "^5.90.3"
  }
}
